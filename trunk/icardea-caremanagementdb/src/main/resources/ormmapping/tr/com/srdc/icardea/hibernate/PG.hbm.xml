<hibernate-mapping>
  <class lazy="false" name="tr.com.srdc.icardea.hibernate.PG" table="PG">
    <id column="ID" name="ID" type="integer" unsaved-value="0">
      <generator class="increment">
			</generator>
    </id>
    <many-to-one access="field" cascade="save-update,lock" class="tr.com.srdc.icardea.hibernate.System" column="SystemID" lazy="no-proxy" name="system" not-null="true" unique="true">
		</many-to-one>
    <property column="type" lazy="false" length="50" name="type" not-null="false" type="string"/>
    <property column="model" lazy="false" length="50" name="model" not-null="false" type="string"/>
    <property column="serial" lazy="false" length="50" name="serial" not-null="false" type="string"/>
    <property column="mfg" lazy="false" length="50" name="mfg" not-null="false" type="string"/>
    <property column="implant_dt" lazy="false" length="50" name="implant_dt" not-null="false" type="string"/>
    <property column="implanter" lazy="false" length="50" name="implanter" not-null="false" type="string"/>
    <property column="implanter_contact_info" lazy="false" length="50" name="implanter_contact_info" not-null="false" type="string"/>
    <property column="implanting_facility" lazy="false" length="50" name="implanting_facility" not-null="false" type="string"/>
  </class>
</hibernate-mapping>
