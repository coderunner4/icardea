<hibernate-mapping>
  <class lazy="false" name="tr.com.srdc.icardea.hibernate.PersonalizedMedicalCareplanInstance" table="PersonalizedMedicalCareplanInstance">
    <id column="ID" name="ID" type="integer" unsaved-value="0">
      <generator class="increment">
			</generator>
    </id>
    <property column="Content" lazy="false" name="content" not-null="false" type="text"/>
    <property column="Identifier" lazy="false" length="32" name="identifier" not-null="false" type="string"/>
    <property column="InstantiationDate" lazy="false" name="instantiationDate" not-null="false" type="timestamp"/>
    <many-to-one access="field" cascade="lock" class="tr.com.srdc.icardea.hibernate.PersonalizedMedicalCareplan" column="PersonalizedMedicalCareplanID" lazy="no-proxy" name="personalizedMedicalCareplan" not-null="true">
		</many-to-one>
  </class>
</hibernate-mapping>
