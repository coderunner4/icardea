<?xml version="1.0" encoding="UTF-8"?>
<service id="remoting-service" class="flex.messaging.services.RemotingService">

    <adapters>
        <adapter-definition id="java-object" class="flex.messaging.services.remoting.adapters.JavaAdapter" default="true"/>
    </adapters>
    
    <destination id="helloWorldService">
    	<properties>
    		<factory>spring</factory>
    		<source>helloWorldService</source>
    	</properties>
    </destination>
    
    <destination id="medicalcareplantemplateDAO">
    	<properties>
    		<factory>spring</factory>
    		<source>medicalcareplantemplateDAO</source>
    	</properties>
    </destination>
    
    <destination id="personalizedmedicalcareplanDAO">
    	<properties>
    		<factory>spring</factory>
    		<source>personalizedmedicalcareplanDAO</source>
    	</properties>
    </destination>
    
    <destination id="patientDAO">
    	<properties>
    		<factory>spring</factory>
    		<source>patientDAO</source>
    	</properties>
    </destination>
	
    <default-channels>
        <channel ref="my-amf"/>
    </default-channels>

</service>
