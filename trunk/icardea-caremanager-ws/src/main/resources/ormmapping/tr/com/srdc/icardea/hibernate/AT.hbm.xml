<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="tr.com.srdc.icardea.hibernate.AT" table="AT" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<many-to-one name="sTAT" column="STATID" class="tr.com.srdc.icardea.hibernate.STAT" cascade="save-update,lock" unique="true" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
		<property name="dtm_start" column="dtm_start" type="string" length="50" not-null="false" lazy="false"/>
		<property name="dtm_end" column="dtm_end" type="string" length="50" not-null="false" lazy="false"/>
		<property name="mode_sw_max_duration" column="mode_sw_max_duration" type="string" length="50" not-null="false" lazy="false"/>
		<property name="burden_percent" column="burden_percent" type="string" length="50" not-null="false" lazy="false"/>
		<property name="mode_sw_percent_time" column="mode_sw_percent_time" type="string" length="50" not-null="false" lazy="false"/>
		<property name="mode_sw_percent_time_per_day" column="mode_sw_percent_time_per_day" type="string" length="50" not-null="false" lazy="false"/>
		<property name="mode_sw_count" column="mode_sw_count" type="string" length="50" not-null="false" lazy="false"/>
		<property name="mode_sw_count_per_day" column="mode_sw_count_per_day" type="string" length="50" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
