<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="tr.com.srdc.icardea.hibernate.System" table="System" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<one-to-one name="observation" class="tr.com.srdc.icardea.hibernate.Observation" cascade="save-update,lock" property-ref="system" lazy="no-proxy" access="field"/>
		<one-to-one name="pG" class="tr.com.srdc.icardea.hibernate.PG" cascade="save-update,lock" property-ref="system" lazy="no-proxy" access="field"/>
		<set name="ORM_lEAD2" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="SystemID" not-null="true"/>
			<one-to-many class="tr.com.srdc.icardea.hibernate.LEAD2"/>
		</set>
		<one-to-one name="sESS" class="tr.com.srdc.icardea.hibernate.SESS" cascade="save-update,lock" property-ref="system" lazy="no-proxy" access="field"/>
		<one-to-one name="mSMT" class="tr.com.srdc.icardea.hibernate.MSMT" cascade="save-update,lock" property-ref="system" lazy="no-proxy" access="field"/>
		<one-to-one name="sET" class="tr.com.srdc.icardea.hibernate.SET" cascade="save-update,lock" property-ref="system" lazy="no-proxy" access="field"/>
		<one-to-one name="sTAT" class="tr.com.srdc.icardea.hibernate.STAT" cascade="save-update,lock" property-ref="system" lazy="no-proxy" access="field"/>
		<set name="ORM_ePISODEROOT" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="SystemID" not-null="true"/>
			<one-to-many class="tr.com.srdc.icardea.hibernate.EPISODEROOT"/>
		</set>
	</class>
</hibernate-mapping>
