<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="tr.com.srdc.icardea.hibernate.PG" table="PG" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<many-to-one name="system" column="SystemID" class="tr.com.srdc.icardea.hibernate.System" cascade="save-update,lock" unique="true" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
		<property name="type" column="type" type="string" length="50" not-null="false" lazy="false"/>
		<property name="model" column="model" type="string" length="50" not-null="false" lazy="false"/>
		<property name="serial" column="serial" type="string" length="50" not-null="false" lazy="false"/>
		<property name="mfg" column="mfg" type="string" length="50" not-null="false" lazy="false"/>
		<property name="implant_dt" column="implant_dt" type="string" length="50" not-null="false" lazy="false"/>
		<property name="implanter" column="implanter" type="string" length="50" not-null="false" lazy="false"/>
		<property name="implanter_contact_info" column="implanter_contact_info" type="string" length="50" not-null="false" lazy="false"/>
		<property name="implanting_facility" column="implanting_facility" type="string" length="50" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
