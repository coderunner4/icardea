<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="tr.com.srdc.icardea.hibernate.BinaryData" table="BinaryData" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="base64String" column="base64String" type="text" not-null="false" lazy="false"/>
		<many-to-one name="observation" cascade="lock" column="ObservationID" class="tr.com.srdc.icardea.hibernate.Observation" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
	</class>
</hibernate-mapping>
