<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="tr.com.srdc.icardea.hibernate.BATTERY" table="BATTERY" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<many-to-one name="mSMT" cascade="lock" column="MSMTID" class="tr.com.srdc.icardea.hibernate.MSMT" not-null="false" lazy="no-proxy" access="field">
		</many-to-one>
		<property name="dtm" column="dtm" type="string" length="50" not-null="false" lazy="false"/>
		<property name="status" column="status" type="string" length="50" not-null="false" lazy="false"/>
		<property name="voltage" column="voltage" type="string" length="50" not-null="false" lazy="false"/>
		<property name="impedance" column="impedance" type="string" length="50" not-null="false" lazy="false"/>
		<property name="remaining_longevity" column="remaining_longevity" type="string" length="50" not-null="false" lazy="false"/>
		<property name="remaining_percentage" column="remaining_percentage" type="string" length="50" not-null="false" lazy="false"/>
		<property name="rrt_trigger" column="rrt_trigger" type="string" length="50" not-null="false" lazy="false"/>
		<property name="subID" column="subID" type="integer" length="11" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
