<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="tr.com.srdc.icardea.hibernate.MedicalCareplanTemplate" table="MedicalCareplanTemplate" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<property name="content" column="Content" type="text" not-null="false" lazy="false"/>
		<property name="identifier" column="Identifier" type="string" length="32" not-null="false" lazy="false"/>
		<property name="approved" column="Approved" type="boolean" length="1" not-null="false" lazy="false"/>
		<property name="ICD10Code" column="ICD10Code" type="string" length="20" not-null="false" lazy="false"/>
		<property name="creationDate" column="CreationDate" type="timestamp" not-null="false" lazy="false"/>
		<property name="name" column="Name" type="string" length="100" not-null="false" lazy="false"/>
		<set name="ORM_PersonalizedMedicalCareplan" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="MedicalCareplanTemplateID" not-null="true"/>
			<one-to-many class="tr.com.srdc.icardea.hibernate.PersonalizedMedicalCareplan"/>
		</set>
	</class>
</hibernate-mapping>
