<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="tr.com.srdc.icardea.hibernate.PatientHealthcareActorAssignment" table="PatientHealthcareActorAssignment" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<property name="patientIdentifier" column="PatientIdentifier" type="string" length="32" not-null="false" lazy="false"/>
		<property name="healthcareActorIdentifier" column="HealthcareActorIdentifier" type="string" length="32" not-null="false" lazy="false"/>
		<property name="assignmentDate" column="AssignmentDate" type="timestamp" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
