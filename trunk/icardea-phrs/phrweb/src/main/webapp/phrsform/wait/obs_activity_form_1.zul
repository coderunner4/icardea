	-->
<div >
flagEdit=		<label value="@{winmgtCtrl.flagEdit}"/>
flagNewForm=	<label value="@{winmgtCtrl.flagNewForm}"/>

<groupbox>
	<caption label="${labels.activityItem.label}" />
<button id="save"  			    label="${labels.default.button.create.label}" 	/>		 
