<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="tr.com.srdc.icardea.careplanengine.entities.GEHistoryHeader" table="GEHISTORY_HEADER">
		<id name="sessionID" column="SESSIONID">
			<generator class="increment"/>
		</id>

		<property name="assignmentID" column="ASSIGNMENTID" type="long"/>
		<property name="executionStartTime" column="TSTAMP" type="timestamp"/>

	</class>	
</hibernate-mapping>
