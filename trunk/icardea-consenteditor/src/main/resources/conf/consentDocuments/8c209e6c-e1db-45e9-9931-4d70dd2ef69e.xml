    <tns:consentRule tns:id="12132923489">
        <tns:description>Dentists can update my basic health information and conditions</tns:description>
        <tns:consentRuleTarget>
            <tns:subjectElements>
                <tns:subjectElement>
                    <tns:group tns:isUserDefined="false">
                        <tns:id>ROLECODE:DENTIST</tns:id>
                        <tns:name>Dentist</tns:name>
                        <tns:members />
                    </tns:group>
                </tns:subjectElement>
            </tns:subjectElements>
            <tns:resourceElements>
                <tns:resourceElement>
                    <tns:id>RESOURCECODE:BASICHEALTH</tns:id>
                    <tns:name>Basic Health Information</tns:name>
                    <tns:confidentialityCode>LOW</tns:confidentialityCode>
                </tns:resourceElement>
                <tns:resourceElement>
                    <tns:id>RESOURCECODE:CONDITION</tns:id>
                    <tns:name>Conditions</tns:name>
                    <tns:confidentialityCode>LOW</tns:confidentialityCode>
                </tns:resourceElement>
            </tns:resourceElements>
            <tns:actionElements>
                <tns:action>WRITE</tns:action>
            </tns:actionElements>
        </tns:consentRuleTarget>
        <tns:isAllow>1</tns:isAllow>
        <tns:condition>
            <tns:timeConstraint>
                <tns:allowFrom>2001-12-17 09:30:27</tns:allowFrom>
                <tns:allowUntil>2011-12-20 19:30:27</tns:allowUntil>
