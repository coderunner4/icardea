<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="tr.com.srdc.icardea.hibernate.Encounter" table="Encounter" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<property name="careProvisionCode" column="careProvisionCode" type="string" length="255" not-null="false" lazy="false"/>
		<property name="encounterCode" column="encounterCode" type="string" length="255" not-null="false" lazy="false"/>
		<property name="effectiveTimeLow" column="effectiveTimeLow" type="string" length="255" not-null="false" lazy="false"/>
		<property name="effectiveTimeHigh" column="effectiveTimeHigh" type="string" length="255" not-null="false" lazy="false"/>
		<property name="encounterText" column="encounterText" type="string" length="255" not-null="false" lazy="false"/>
		<property name="doctorID" column="doctorID" type="string" length="255" not-null="false" lazy="false"/>
		<property name="organizationID" column="organizationID" type="string" length="255" not-null="false" lazy="false"/>
		<many-to-one name="eHRPHRData" column="EHRPHRDataID" class="tr.com.srdc.icardea.hibernate.EHRPHRData" cascade="save-update,lock" unique="true" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
	</class>
</hibernate-mapping>
