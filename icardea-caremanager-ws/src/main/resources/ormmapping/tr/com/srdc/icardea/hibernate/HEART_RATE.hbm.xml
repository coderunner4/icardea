<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="tr.com.srdc.icardea.hibernate.HEART_RATE" table="HEART_RATE" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<many-to-one name="sTAT" column="STATID" class="tr.com.srdc.icardea.hibernate.STAT" cascade="save-update,lock" unique="true" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
		<property name="dtm_start" column="dtm_start" type="string" length="50" not-null="false" lazy="false"/>
		<property name="dtm_end" column="dtm_end" type="string" length="50" not-null="false" lazy="false"/>
		<property name="atrial_min" column="atrial_min" type="string" length="50" not-null="false" lazy="false"/>
		<property name="atrial_max" column="atrial_max" type="string" length="50" not-null="false" lazy="false"/>
		<property name="atrial_mean" column="atrial_mean" type="string" length="50" not-null="false" lazy="false"/>
		<property name="ventricular_min" column="ventricular_min" type="string" length="50" not-null="false" lazy="false"/>
		<property name="ventricular_max" column="ventricular_max" type="string" length="50" not-null="false" lazy="false"/>
		<property name="ventricular_mean" column="ventricular_mean" type="string" length="50" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
