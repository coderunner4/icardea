<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="tr.com.srdc.icardea.hibernate.Contact" table="Contact" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<property name="addressLine" column="AddressLine" type="string" length="255" not-null="false" lazy="false"/>
		<property name="faxNumber" column="FaxNumber" type="string" length="20" not-null="false" lazy="false"/>
		<property name="email" column="Email" type="string" length="100" not-null="false" lazy="false"/>
		<property name="phoneNumber" column="PhoneNumber" type="string" length="20" not-null="false" lazy="false"/>
		<property name="mobileNumber" column="MobileNumber" type="string" length="20" not-null="false" lazy="false"/>
		<property name="GTalk" column="GTalk" type="string" length="20" not-null="false" lazy="false"/>
		<many-to-one name="organization" column="OrganizationID" class="tr.com.srdc.icardea.hibernate.Organization" cascade="save-update,lock" unique="true" not-null="false" lazy="no-proxy" access="field">
		</many-to-one>
		<many-to-one name="person" column="PersonID" class="tr.com.srdc.icardea.hibernate.Person" cascade="save-update,lock" unique="true" not-null="false" lazy="no-proxy" access="field">
		</many-to-one>
	</class>
</hibernate-mapping>
